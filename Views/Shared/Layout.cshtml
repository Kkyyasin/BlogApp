<!DOCTYPE html>
<html>

<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <title>@ViewBag.Title - My Blog</title>
    <link href="~/lib/bootstrap/css/bootstrap.min.css" rel="stylesheet">
    <link href="~/css/site.css" rel="stylesheet">
    <link href="~/lib/bootstrap-icon/font/bootstrap-icons.min.css" rel="stylesheet">

</head>


<body>

    <header>
        @Html.Partial("_Navbar")
    </header>
    <div class="container mt-3">
        @RenderBody()
    </div>
    <footer class="footer bg-light mt-auto py-3 mt-5">
        <div class="container content-containe">
            <span class="text-muted">Place sticky footer content here.</span>
        </div>
    </footer>


    <script src="~/lib/jquery/jquery.min.js"></script>
    <script src="~/lib/popper/umd/popper.min.js"></script>
    <script src="~/lib/bootstrap/js/bootstrap.min.js"></script>
    <script src="~/lib/sweetalert2/sweetalert2.js"></script>
    @RenderSection("Scripts",required:false)

</body>

</html>